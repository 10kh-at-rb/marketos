<!DOCTYPE html>
<html>
<head>
  <title>Marketos</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container-fluid">
  <div class="row-fluid">
    <div class="col-md-12">
      <%= render 'layouts/header' %>
    </div>
    <div class="col-md-2">
      <%= render 'layouts/sidebar' %> 
    </div>
    <div class="col-md-10">
      <div class="row">
        <%= render 'layouts/sale_items' %> 

        <%= yield %>
      </div>
    </div>
  </div>
</div>

</body>
</html>
